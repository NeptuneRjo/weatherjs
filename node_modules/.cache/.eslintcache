`\n    const abortCont = new AbortController();\n\n    fetch(url, { signal: abortCont.signal })\n      .then(res => {\n        if (!res.ok) {\n          throw Error('could not fetch the data for that resource');\n        }\n        return res.json();\n      })\n      .then(data => {\n        setData(data);\n        setIsPending(false);\n        setError(null);\n      })\n      .catch(err => {\n        if (err.name === 'AbortError') {\n          console.log('fetch aborted')\n        } else {\n          setError(err.message);\n          setIsPending(false);\n        }\n\n      });\n\n      return () => abortCont.abort();\n  }\n\n  const handleLocation = (userLocation) => {\n    const newLocation = userLocation;\n\n    handleFetch(newLocation);\n    handleSessionDefault(newLocation);\n  }\n\n  const handleSessionDefault = (newDefault) => {\n    window.sessionStorage.setItem('location', newDefault);\n  }\n\n  useEffect(() => {\n    if (window.sessionStorage.getItem('location') !== null) {\n      handleFetch(window.sessionStorage.getItem('location'));\n      return;\n    }\n\n    handleFetch(defaultLocation)\n  }, [])\n\n  if (isPending) {\n    return (\n      <div className=\"app-loading\">\n        <p>Loading...</p>\n      </div>\n    )\n  }\n  return (\n    <div className=\"app\">\n        <Header handleLocation={handleLocation} />\n        <Body data={data} />\n    </div>\n  )\n}\n\nexport default App","/home/neptunerjo/personal-projects/weather-app/src/components/Header/Header.jsx",[],[],["28","29"],"/home/neptunerjo/personal-projects/weather-app/src/components/Body/Body.jsx",["30"],[],"import React from 'react'\nimport './styling/body.css'\n\nconst BodyContent = ({ data }) => {\n\n    const kelvinToFarenheit = (kelvin) => (\n        ((kelvin - 273.15) * (9/5) + 32).toFixed(0)\n    );\n\n    return (\n        <div className=\"body-main\">\n            <div className=\"weather-main\">\n                <div className=\"current-temp\">\n                    <div>{kelvinToFarenheit(data.main.temp)}°f</div>\n                    <p>{data.name}</p>\n                </div>\n                <div className=\"weather-icon\">\n                    <img src={`http://openweathermap.org/img/wn/${data.weather[0].icon}@2x.png`} />\n                    <p>{data.weather[0].description}</p>\n                </div>\n            </div>\n            <div className=\"weather-minmax\">\n                <div className=\"min-temp\">\n                    <div>{kelvinToFarenheit(data.main.temp_min)}°f</div>\n                    <p>Low</p>\n                </div>\n                <div className=\"max-temp\">\n                    <div>{kelvinToFarenheit(data.main.temp_max)}°f</div>\n                    <p>High</p>\n                </div>\n            </div>\n            <div className=\"weather-other\">\n                <div className=\"pressure\">\n                    <div>{data.main.pressure}mb</div>\n                    <p>pressure</p>\n                </div>\n                <div className=\"wind-speed\">\n                    <div>{(data.wind.speed).toFixed(0)} mp/h</div>\n                    <p>wind speed</p>\n                </div>\n                <div className=\"humidity\">\n                    <div>{data.main.humidity}%</div>\n                    <p>humidity</p>\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default BodyContent",{"ruleId":"31","severity":1,"message":"32","line":9,"column":27,"nodeType":"33","messageId":"34","endLine":9,"endColumn":45},{"ruleId":"31","severity":1,"message":"35","line":13,"column":10,"nodeType":"33","messageId":"34","endLine":13,"endColumn":15},{"ruleId":"36","severity":1,"message":"37","line":63,"column":6,"nodeType":"38","endLine":63,"endColumn":8,"suggestions":"39"},{"ruleId":"40","replacedBy":"41"},{"ruleId":"42","replacedBy":"43"},{"ruleId":"44","severity":1,"message":"45","line":18,"column":21,"nodeType":"46","endLine":18,"endColumn":100},"no-unused-vars","'setDefaultLocation' is